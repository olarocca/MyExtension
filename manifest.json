{
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "js": [ "popup.js" ],
      "matches": [ "*://*/*" ]
    }
  ],
  "description": "This extension will analyze a page using GTmetrix",
  "manifest_version": 2,
  "name": "Hello World",
  "permissions": [
    "activeTab"
  ],
  "version": "1.0"
}